{
  "name": "learning-tracker-app",
  "version": "1.0.0",
  "description": "Full-stack learning tracker application",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "vite",
    "build": "vite build",
    "start": "node server.js",
    "generate-template": "node scripts/make_xlsx.js",
    "preview": "vite preview",
    "docker:build": "docker build -t docker.io/rajeshkr2025/learning-tracker:latest .",
    "docker:push": "docker push docker.io/rajeshkr2025/learning-tracker:latest",
    "docker:build-push": "npm run docker:build && npm run docker:push",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "keywords": [
    "learning",
    "tracker",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "xlsx": "^0.18.5",
    "mongodb": "^5.9.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "lucide-react": "^0.263.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.8"
  }
}
